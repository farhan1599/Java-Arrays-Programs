package com.arrays.practice;

import java.util.HashSet;
import java.util.Set;

public class RemoveDuplicate {

	public static void main(String[] args) {

		int[] arr = { 2, 3, 2, 1, 1,5,4,5,6 ,45,65,45,44,6,6,45,65};

		removeDuplicateElements(arr);
	}

	private static void removeDuplicateElements(int[] arr) {
		System.out.println("Before removing :");
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i] + " ");
		}
		System.out.println();
		//Set<Integer> set = new TreeSet<>(); //if you want sorted output
		Set<Integer> set = new HashSet<>();

		for (int num : arr) {
			set.add(num);

		}
		
		Integer[] arr2 =set.toArray(new Integer[0]);
		
		for(int i=0;i<arr2.length;i++) {
			System.out.println(arr2[i]);
		}
//		System.out.println("After removing :");
//		for (int num : set) {
//			System.out.print(num + " ");

		//}

	}

}
