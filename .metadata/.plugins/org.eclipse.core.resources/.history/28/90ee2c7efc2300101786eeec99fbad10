package com.arrays.practice;

public class SecondLargest {

	public static void main(String[] args) {

		int[] arr = { 23, 34, 12, 53, 54,66 };

		findSecondMax(arr);
	}

	private static void findSecondMax(int[] arr) {
		int large = arr[0];
		//int second_large = arr[0];

		for (int i = 1; i < arr.length; i++) {
 
			if(arr[i]>large) {
				//second_large = large;
				large=arr[i];
						
			}//else if(arr[i]!=large && arr[i]>second_large) {
			//	second_large=arr[i];
		//	}
		}
		System.out.println("Second large number is :"+large);

	}

}
